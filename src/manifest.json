{
  "name": "Review common util helper",
  "version": "1.0",
  "description": "When life gives you reviews",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "run_at": "document_idle",
    "js": ["contentScript.js"]
  }]

}
