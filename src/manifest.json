{
  "name": "Review common util helper",
  "version": "1.0",
  "description": "When life gives you reviews",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "externally_connectable": {
    "ids": ["*"],
    "matches": [
      "https://git.consdata.local/*"
    ]
  },
  "permissions": [
    "storage",
    "https://git.consdata.local/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://git.consdata.local/*"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
